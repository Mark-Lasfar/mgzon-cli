      "mainEntity": [
        {
          "@type": "Question",
          "name": "What is MGZON CLI?",
          "acceptedAnswer": {
            "@type": "Answer",
            "text": "MGZON CLI is a professional command-line interface tool for the MGZON ecommerce platform. It helps developers create, deploy, and manage applications with interactive terminal simulation."
          }
        },
        {
          "@type": "Question",
          "name": "Which platforms are supported?",
          "acceptedAnswer": {
            "@type": "Answer",
            "text": "MGZON CLI supports Linux (AppImage), macOS (.dmg), and Windows (.exe). It also works via npm for Node.js developers."
          }
        },
        {
          "@type": "Question",
          "name": "How do I install MGZON CLI?",
          "acceptedAnswer": {
            "@type": "Answer",
            "text": "You can install via npm: 'npm install -g @mg-cli/cli' or download standalone binaries from GitHub Releases."
          }
        },
        {
          "@type": "Question",
          "name": "Is MGZON CLI free?",
          "acceptedAnswer": {
            "@type": "Answer",
            "text": "Yes, MGZON CLI is completely free and open source under MIT license."
          }
        },
        {
          "@type": "Question",
          "name": "How do I get started?",
          "acceptedAnswer": {
            "@type": "Answer",
            "text": "1. Install CLI 2. Run 'mz login' 3. Create app with 'mz init my-app' 4. Deploy with 'mz deploy'"
          }
        }
      ]
